{
  "name": "crwn-clothing-server",
  "version": "1.0.0",
  "engines": {
    "node": "14.15.5",
    "npm": "7.6.3"
  },
  "scripts": {
    "client": "cd crwn-clothing && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "stripe": "8.145.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.2"
  }
}
